{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./src/built",
        "rootDir": "./src",
        "types": ["vscode", "node"],
        "typeRoots": [
            "node_modules/@types"
        ],
        "strict": true,
        "noImplicitAny": false
    },
    "include": [
        "./src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ],
     "moduleResolution": "node"
}
